doctype html
html(lang="en")
    include ../mixins/message
    include ../partials/head
    body 
        if messages.info 
            +message("info", messages.info)
        if messages.error 
            +message("error", messages.error)
        include ../partials/header
        div.flex-container
            include ../partials/sidebar
            main#main
                block content
        include ../partials/footer
    block scripts